/* Q:69 Find the second largest element in an array */
#include <stdio.h>

int main()
{
    int arr[100], n, i, first, second;
    scanf("%d", &n);
    for(i = 0; i < n; i++)
{
        scanf("%d", &arr[i]);
    }
    if(n < 2)
{
        printf("Not enough elements");
        return 0;
    }
    first = second = -2147483648;
    for(i = 0; i < n; i++) {
        if(arr[i] > first) {
            second = first;
            first = arr[i];
        } else if(arr[i] > second && arr[i] < first) {
            second = arr[i];
        }
    }
    if(second == -2147483648) {
        printf("No second largest element");
    } else {
        printf("%d", second);
    }
    return 0;
}
