/*Create Employee structure with nested Date structure for joining date and print details.*/
#include <stdio.h>

struct Date {
    int day;
    int month;
    int year;
};

struct Employee {
    char name[50];
    int emp_id;
    float salary;
    struct Date joining_date;
};

int main() {
    struct Employee emp;

    scanf(" %[^\n]", emp.name);
    scanf("%d", &emp.emp_id);
    scanf("%f", &emp.salary);
    scanf("%d %d %d", &emp.joining_date.day, &emp.joining_date.month, &emp.joining_date.year);

    printf("%s\n", emp.name);
    printf("%d\n", emp.emp_id);
    printf("%.2f\n", emp.salary);
    printf("%02d-%02d-%04d\n", emp.joining_date.day, emp.joining_date.month, emp.joining_date.year);

    return 0;
}
